@{
    ViewBag.Title = "API";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div id="content"></div>

@*@section Scripts{
        <script src="~/Scripts/react/react-0.12.2.min.js"></script>
        <script src="@Url.Content("~/Scripts/Tweets.jsx")"></script>


    }*@


<h2>@ViewBag.CompanyName Current Tweets</h2>

<table class="table table-hover table-bordered">
    <tbody>

        @foreach (dynamic item in Model)
        {
            <tr>
                <td><a href='@item.Url' target="_blank">@item.Text</a> </td>
            </tr>
        }

    </tbody>
</table>

<a href='https://twitter.com/@ViewBag.CompanyName' target="_blank">View More</a>